<form action="/cats" method="POST">
  <label>Name
    <input type="text" name="cat[name]" value="<%= @cat.name %>">
  </label>

  <label>Owner ID
    <select name="cat[owner_id]">
      <% @human.each do |p| %>
        <option value="<%= p.id %>"><%= p.name %></option>
      <% end %>
    </select>
  </label>

  <input type="submit">
</form>
